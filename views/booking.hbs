<main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">
    <div class="container-fluid py-4">
        <div class="row">
            <div class="col-12">
                <div class="card mb-4">
                    <div class="card-header pb-0">
                        <h6>Booking</h6>
                    </div>
                    <div class="card-body px-0 pt-0 pb-2">
                        <div class="table-responsive p-0">
                            <table class="table align-items-center mb-0">
                                <thead>
                                    <tr>
                                        <th class="text-center text-secondary opacity-7">
                                            User</th>
                                        <th class="text-center text-secondary opacity-7">
                                            Email</th>
                                        <th class="text-center text-secondary opacity-7">
                                            Phone Number</th>
                                        <th class="text-center text-secondary opacity-7">
                                            Seat</th>
                                        <th class="text-center text-secondary opacity-7">
                                            Time</th>
                                        <th class="text-center text-secondary opacity-7">
                                            Day Booking</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each booking}}
                                    <tr>
                                        <td>
                                            <p class="text-center text-xs font-weight-bold mb-0">{{user_id.name}}</p>
                                        </td>
                                        <td>
                                            <p class="text-center text-xs font-weight-bold mb-0">{{user_id.email}}
                                            </p>
                                        </td>
                                        <td>
                                            <p class="text-center text-xs font-weight-bold mb-0">{{user_id.phoneNumber}}
                                            </p>
                                        </td>
                                        <td>
                                            <p class="text-center text-xs font-weight-bold mb-0">{{seat}}
                                            </p>
                                        </td>
                                        <td>
                                            <p class="text-center text-xs font-weight-bold mb-0">
                                                {{table_id.timeline_id.name}}</p>
                                        </td>
                                        <td>
                                            <p class="text-center text-xs font-weight-bold mb-0">{{dayBooking}}</p>
                                        </td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>